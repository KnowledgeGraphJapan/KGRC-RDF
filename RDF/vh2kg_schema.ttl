@prefix : <http://example.org/virtualhome2kg/ontology/> .
@prefix ac: <http://example.org/virtualhome2kg/ontology/action/> .
@prefix ho: <http://www.owl-ontologies.com/VirtualHome.owl#> .
@prefix ob: <http://raw.githubusercontent.com/aistairc/HomeObjectOntology/main/HomeObject.owl#> .
@prefix hra: <http://example.org/virtualhome2kg/ontology/homeriskactivity/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix x3do: <https://www.web3d.org/specifications/X3dOntology4.0#> .
@base <http://example.org/virtualhome2kg/ontology/> .

<http://example.org/virtualhome2kg/ontology/> rdf:type owl:Ontology ;
                                               rdfs:comment """This ontology offers vocabularies for representing activities of daily living (ADL) that can be run in the VirtualHome (http://virtual-home.org/), run-time home situations, objects, and relationships among them.
This ontology is an extended version of the HomeOntology (https://github.com/valexande/HomeOntology).""" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#closeMatch
<http://www.w3.org/2004/02/skos/core#closeMatch> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  https://www.web3d.org/specifications/X3dOntology4.0#SFDouble
x3do:SFDouble rdf:type rdfs:Datatype .


###  https://www.web3d.org/specifications/X3dOntology4.0#SFVec3f
x3do:SFVec3f rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://example.org/virtualhome2kg/ontology/action
:action rdf:type owl:ObjectProperty ;
        rdfs:domain :Event ;
        rdfs:range :Action .


###  http://example.org/virtualhome2kg/ontology/activity
:activity rdf:type owl:ObjectProperty ;
          rdfs:domain <http://plod.info/rdf/Episode> ;
          rdfs:range ho:Activity .


###  http://example.org/virtualhome2kg/ontology/affords
:affords rdf:type owl:ObjectProperty ;
         rdfs:domain :Object ;
         rdfs:range :Action .


###  http://example.org/virtualhome2kg/ontology/agent
:agent rdf:type owl:ObjectProperty ;
       rdfs:domain :Event ;
       rdfs:range :Character ;
       <http://www.w3.org/2004/02/skos/core#closeMatch> <http://purl.org/NET/c4dm/event.owl#agent> .


###  http://example.org/virtualhome2kg/ontology/attribute
:attribute rdf:type owl:ObjectProperty ;
           rdfs:domain :Object ;
           rdfs:range :Attribute .


###  http://example.org/virtualhome2kg/ontology/bbox
:bbox rdf:type owl:ObjectProperty ;
      owl:inverseOf :isBboxOf ;
      rdfs:domain :State ;
      rdfs:range x3do:Shape ;
      rdfs:comment "3D bounding box" .


###  http://example.org/virtualhome2kg/ontology/between
:between rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :relatedShape ;
         rdfs:domain x3do:Shape ;
         rdfs:range x3do:Shape ;
         rdfs:comment "used for door object, if door is between kitchen and livingroom, we have edges <door, between, livingroom> and <door, between ,kitchen>" .


###  http://example.org/virtualhome2kg/ontology/close
:close rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf :relatedShape ;
       rdfs:domain x3do:Shape ;
       rdfs:range x3do:Shape ;
       rdfs:comment "edge <object1, close, object2> denotes that the distance between center of object1 (object2) to the bounding box of object2 (object1) is < 1.5 units (~meters)" .


###  http://example.org/virtualhome2kg/ontology/facing
:facing rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relatedShape ;
        rdfs:domain x3do:Shape ;
        rdfs:range x3do:Shape ;
        rdfs:comment "there is an edge <object1, facing, object2> if object2 is lookable, is visible from object1, and the distance between the centers is < 5 units (~meters). If object1 is a sofa or a chair it should also be turned towards object2" .


###  http://example.org/virtualhome2kg/ontology/from
:from rdf:type owl:ObjectProperty ;
      rdfs:domain :Event ;
      rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
             rdfs:domain <http://plod.info/rdf/Episode> ;
             rdfs:range ho:Activity .


###  http://example.org/virtualhome2kg/ontology/hasEvent
:hasEvent rdf:type owl:ObjectProperty ;
          rdfs:domain ho:Activity ;
          rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/height
:height rdf:type owl:ObjectProperty .


###  http://example.org/virtualhome2kg/ontology/holds_lh
:holds_lh rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedShape ;
          rdfs:domain x3do:Shape ;
          rdfs:range x3do:Shape ;
          rdfs:comment "analogue of holds_rh for left hand" .


###  http://example.org/virtualhome2kg/ontology/holds_rh
:holds_rh rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :relatedShape ;
          rdfs:domain x3do:Shape ;
          rdfs:range x3do:Shape ;
          rdfs:comment "edge <character, holds_rh, object> is used to indicate that character holds an object in its right hand" .


###  http://example.org/virtualhome2kg/ontology/inside
:inside rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relatedShape ;
        rdfs:domain x3do:Shape ;
        rdfs:range x3do:Shape .


###  http://example.org/virtualhome2kg/ontology/isBboxOf
:isBboxOf rdf:type owl:ObjectProperty ;
          rdfs:domain x3do:Shape .


###  http://example.org/virtualhome2kg/ontology/isStateOf
:isStateOf rdf:type owl:ObjectProperty ;
           rdfs:domain :State ;
           rdfs:range :Object .


###  http://example.org/virtualhome2kg/ontology/mainObject
:mainObject rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ho:object ;
            rdfs:domain :Event ;
            rdfs:range :Object ;
            rdfs:comment """A main object of an event.
If an agent pours milk into a glass, the value of the mainObject is the milk.""" .


###  http://example.org/virtualhome2kg/ontology/nextActivity
:nextActivity rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :relatedActivity ;
              rdfs:domain ho:Activity ;
              rdfs:range ho:Activity .


###  http://example.org/virtualhome2kg/ontology/nextEvent
:nextEvent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedEvent ;
           rdfs:domain :Event ;
           rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/nextSituation
:nextSituation rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedSituation ;
               owl:inverseOf :previousSituation ;
               rdfs:domain :Situation ;
               rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/nextState
:nextState rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :relatedState ;
           owl:inverseOf :previousState ;
           rdfs:domain :State ;
           rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/on
:on rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :relatedShape ;
    rdfs:domain x3do:Shape ;
    rdfs:range x3do:Shape .


###  http://example.org/virtualhome2kg/ontology/partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain :State ;
        rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/place
:place rdf:type owl:ObjectProperty ;
       rdfs:domain :Event ;
       rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/previousActivity
:previousActivity rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :relatedActivity .


###  http://example.org/virtualhome2kg/ontology/previousEvent
:previousEvent rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedEvent ;
               rdfs:domain :Event ;
               rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/previousSituation
:previousSituation rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :relatedSituation ;
                   rdfs:domain :Situation ;
                   rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/previousState
:previousState rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :relatedState ;
               rdfs:domain :State ;
               rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/relatedActivity
:relatedActivity rdf:type owl:ObjectProperty ;
                 rdfs:domain ho:Activity ;
                 rdfs:range ho:Activity .


###  http://example.org/virtualhome2kg/ontology/relatedEvent
:relatedEvent rdf:type owl:ObjectProperty ;
              rdfs:domain :Event ;
              rdfs:range :Event .


###  http://example.org/virtualhome2kg/ontology/relatedShape
:relatedShape rdf:type owl:ObjectProperty ;
              rdfs:domain x3do:Shape ;
              rdfs:range x3do:Shape .


###  http://example.org/virtualhome2kg/ontology/relatedSituation
:relatedSituation rdf:type owl:ObjectProperty ;
                  rdfs:domain :Situation ;
                  rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/relatedState
:relatedState rdf:type owl:ObjectProperty ;
              rdfs:domain :State ;
              rdfs:range :State .


###  http://example.org/virtualhome2kg/ontology/situationAfterEvent
:situationAfterEvent rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :relatedSituation ;
                     rdfs:domain :Event ;
                     rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/situationBeforeEvent
:situationBeforeEvent rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :relatedSituation ;
                      rdfs:domain :Event ;
                      rdfs:range :Situation .


###  http://example.org/virtualhome2kg/ontology/state
:state rdf:type owl:ObjectProperty ;
       rdfs:domain :State ;
       rdfs:range :StateType .


###  http://example.org/virtualhome2kg/ontology/targetObject
:targetObject rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ho:object ;
              rdfs:domain :Event ;
              rdfs:range :Object ;
              rdfs:comment """A target object of an event.
If an agent pours milk into a glass, the value of the value of the targetObject is the glass.""" .


###  http://example.org/virtualhome2kg/ontology/time
:time rdf:type owl:ObjectProperty ;
      rdfs:domain :Event ;
      rdfs:range [ rdf:type owl:Class ;
                   owl:unionOf ( <http://www.w3.org/2006/time#TemporalDuration>
                                 <http://www.w3.org/2006/time#TemporalEntity>
                               )
                 ] ;
      <http://www.w3.org/2004/02/skos/core#closeMatch> <http://purl.org/NET/c4dm/event.owl#time> .


###  http://example.org/virtualhome2kg/ontology/to
:to rdf:type owl:ObjectProperty ;
    rdfs:domain :Event ;
    rdfs:range :Room .


###  http://example.org/virtualhome2kg/ontology/unit
:unit rdf:type owl:ObjectProperty ;
      rdfs:range :Unit .


###  http://example.org/virtualhome2kg/ontology/virtualHome
:virtualHome rdf:type owl:ObjectProperty ;
             rdfs:range :VirtualHome .


###  http://example.org/virtualhome2kg/ontology/weight
:weight rdf:type owl:ObjectProperty .


###  http://www.owl-ontologies.com/VirtualHome.owl#object
ho:object rdf:type owl:ObjectProperty ;
          rdfs:domain :Event ;
          rdfs:range :Object .


###  http://www.w3.org/2006/time#hasDuration
<http://www.w3.org/2006/time#hasDuration> rdf:type owl:ObjectProperty ;
                                          rdfs:range <http://www.w3.org/2006/time#Duration> .


###  http://www.w3.org/2006/time#unitType
<http://www.w3.org/2006/time#unitType> rdf:type owl:ObjectProperty ;
                                       rdfs:range <http://www.w3.org/2006/time#TemporalUnit> .


#################################################################
#    Data properties
#################################################################

###  http://example.org/virtualhome2kg/ontology/actionNumber
:actionNumber rdf:type owl:DatatypeProperty ;
              rdfs:domain :Action ;
              rdfs:range xsd:int .


###  http://example.org/virtualhome2kg/ontology/activityNumber
:activityNumber rdf:type owl:DatatypeProperty ;
                rdfs:domain ho:Activity ;
                rdfs:range xsd:int .


###  http://example.org/virtualhome2kg/ontology/eventNumber
:eventNumber rdf:type owl:DatatypeProperty ;
             rdfs:domain :Event ;
             rdfs:range xsd:int .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#first
rdf:first rdf:type owl:DatatypeProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#rest
rdf:rest rdf:type owl:DatatypeProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#value
rdf:value rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2006/time#numericDuration
<http://www.w3.org/2006/time#numericDuration> rdf:type owl:DatatypeProperty ;
                                              rdfs:range xsd:decimal .


###  https://www.web3d.org/specifications/X3dOntology4.0#bboxCenter
x3do:bboxCenter rdf:type owl:DatatypeProperty ;
                rdfs:domain x3do:Shape ;
                rdfs:range x3do:SFVec3f .


###  https://www.web3d.org/specifications/X3dOntology4.0#bboxSize
x3do:bboxSize rdf:type owl:DatatypeProperty ;
              rdfs:domain x3do:Shape ;
              rdfs:range x3do:SFVec3f .


#################################################################
#    Classes
#################################################################

###  http://example.org/virtualhome2kg/ontology/Action
:Action rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/Attribute
:Attribute rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/Back_Porch
:Back_Porch rdf:type owl:Class ;
            rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Backyard
:Backyard rdf:type owl:Class ;
          rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Bathroom
:Bathroom rdf:type owl:Class ;
          rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Bedroom
:Bedroom rdf:type owl:Class ;
         rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Character
:Character rdf:type owl:Class ;
           rdfs:subClassOf :Object .


###  http://example.org/virtualhome2kg/ontology/Dining_room
:Dining_room rdf:type owl:Class ;
             rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/EndEvent
:EndEvent rdf:type owl:Class ;
          rdfs:subClassOf :Event .


###  http://example.org/virtualhome2kg/ontology/Entrance
:Entrance rdf:type owl:Class ;
          rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Event
:Event rdf:type owl:Class ;
       <http://www.w3.org/2004/02/skos/core#closeMatch> <http://purl.org/NET/c4dm/event.owl#Event> ,
                                                        <http://semanticweb.cs.vu.nl/2009/11/sem/Event> .


###  http://example.org/virtualhome2kg/ontology/Family_Altar
:Family_Altar rdf:type owl:Class ;
              rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Home
:Home rdf:type owl:Class ;
      rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Home_office
:Home_office rdf:type owl:Class ;
             rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Kitchen
:Kitchen rdf:type owl:Class ;
         rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Livingroom
:Livingroom rdf:type owl:Class ;
            rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Object
:Object rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/Room
:Room rdf:type owl:Class ;
      rdfs:subClassOf :Object .


###  http://example.org/virtualhome2kg/ontology/Service_Entrance
:Service_Entrance rdf:type owl:Class ;
                  rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Situation
:Situation rdf:type owl:Class ;
           rdfs:comment "The class which describes the situation of the entire home at a certain moment." .


###  http://example.org/virtualhome2kg/ontology/Stairs
:Stairs rdf:type owl:Class ;
        rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/StartEvent
:StartEvent rdf:type owl:Class ;
            rdfs:subClassOf :Event .


###  http://example.org/virtualhome2kg/ontology/State
:State rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/StateType
:StateType rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/Toilet
:Toilet rdf:type owl:Class ;
        rdfs:subClassOf :Room .


###  http://example.org/virtualhome2kg/ontology/Unit
:Unit rdf:type owl:Class .


###  http://example.org/virtualhome2kg/ontology/VirtualHome
:VirtualHome rdf:type owl:Class .


###  http://plod.info/rdf/Episode
<http://plod.info/rdf/Episode> rdf:type owl:Class .


###  http://www.owl-ontologies.com/VirtualHome.owl#Activity
ho:Activity rdf:type owl:Class .


###  http://www.owl-ontologies.com/VirtualHome.owl#BedTimeSleep
ho:BedTimeSleep rdf:type owl:Class ;
                owl:equivalentClass [ rdf:type owl:Class ;
                                      owl:unionOf ( [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasEvent ;
                                                      owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :action ;
                                                                           owl:hasValue ac:sleep
                                                                         ]
                                                    ]
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty :hasEvent ;
                                                      owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                           owl:onProperty :action ;
                                                                           owl:hasValue ac:wakeUp
                                                                         ]
                                                    ]
                                                  )
                                    ] ;
                rdfs:subClassOf ho:Activity ;
                rdfs:comment """Class with sequences for getting ready for the bed, or get ready for 
sleep, or even put the kids to sleep
.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#Dance
ho:Dance rdf:type owl:Class ;
         rdfs:subClassOf ho:Leisure ;
         rdfs:comment "The class with sequence of actions for dancing." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Drink
ho:Drink rdf:type owl:Class ;
         rdfs:subClassOf ho:EatingDrinking ;
         rdfs:comment """Class that contains sequences where the goal is for the user to drink 
under different circumstances.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#Eat
ho:Eat rdf:type owl:Class ;
       rdfs:subClassOf ho:EatingDrinking ;
       rdfs:comment """Class with subclasses, for eating seqeunces under different 
circumstances.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#EatingDrinking
ho:EatingDrinking rdf:type owl:Class ;
                  owl:equivalentClass [ rdf:type owl:Class ;
                                        owl:unionOf ( [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasEvent ;
                                                        owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :action ;
                                                                             owl:hasValue ac:drink
                                                                           ]
                                                      ]
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :hasEvent ;
                                                        owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                             owl:onProperty :action ;
                                                                             owl:hasValue ac:eat
                                                                           ]
                                                      ]
                                                    )
                                      ] ;
                  rdfs:subClassOf ho:Activity ;
                  rdfs:comment "Class that contains sequences for eating and drinking." .


###  http://www.owl-ontologies.com/VirtualHome.owl#FoodPreparation
ho:FoodPreparation rdf:type owl:Class ;
                   rdfs:subClassOf ho:Activity ;
                   rdfs:comment """Class for preparing food, or other kitchen based acitivities for eating 
or drinking (prepeare coffee).""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#GettingReady
ho:GettingReady rdf:type owl:Class ;
                rdfs:subClassOf ho:Activity ;
                rdfs:comment """Class with sequences for getting ready to go to work, or getting ready 
o perform a task outside the home, or even a procedure that needs prior 
preparation.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#HouseArrangement
ho:HouseArrangement rdf:type owl:Class ;
                    rdfs:subClassOf ho:Activity ;
                    rdfs:comment "Superclass that contains sequence for arrangement tasks inside the house. Some of these classes could have been classified as a House Cleaning subclass. We prefer this hierarchy, because the task are more to the category of organizing object inside a domestic environment, rather than cleaning." .


###  http://www.owl-ontologies.com/VirtualHome.owl#HouseCleaning
ho:HouseCleaning rdf:type owl:Class ;
                 rdfs:subClassOf ho:Activity ;
                 rdfs:comment "The class that contains sequence of actions, with Cleaning tasks." .


###  http://www.owl-ontologies.com/VirtualHome.owl#HygieneStyling
ho:HygieneStyling rdf:type owl:Class ;
                  rdfs:subClassOf ho:Activity ;
                  rdfs:comment """Class for sequence that are related to Hygiene otr Styling (NOTICE: some 
of this sequences could be conisdered also as cleaning).""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#Laugh
ho:Laugh rdf:type owl:Class ;
         rdfs:subClassOf ho:Other ;
         rdfs:comment "The class with sequence of actions for laughing." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Leisure
ho:Leisure rdf:type owl:Class ;
           rdfs:subClassOf ho:Activity ;
           rdfs:comment "Class that contains procedures about leisure" .


###  http://www.owl-ontologies.com/VirtualHome.owl#Other
ho:Other rdf:type owl:Class ;
         rdfs:subClassOf ho:Activity ;
         rdfs:comment """This class contains instances that are tasks performed inside a domestic 
envronmnet but could not be classified in a specific class.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#PhysicalActivity
ho:PhysicalActivity rdf:type owl:Class ;
                    rdfs:subClassOf ho:Activity ;
                    rdfs:comment "Class with sequences for exercise and different physical activities." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Play
ho:Play rdf:type owl:Class ;
        rdfs:subClassOf ho:Leisure ;
        rdfs:comment "The class with sequence of actions on how to play (physical games)." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Read
ho:Read rdf:type owl:Class ;
        rdfs:subClassOf ho:Other ;
        rdfs:comment "The class with sequence of actions for  reading." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Sew
ho:Sew rdf:type owl:Class ;
       rdfs:subClassOf ho:Other ;
       rdfs:comment "The class with sequence of actions for sewing, from inside the house." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Shave
ho:Shave rdf:type owl:Class ;
         rdfs:subClassOf ho:HygieneStyling ;
         rdfs:comment "The class with sequence of actions for shaving your own face." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Sit
ho:Sit rdf:type owl:Class ;
       rdfs:subClassOf ho:Other ;
       rdfs:comment "The class with sequence of actions for sitting." .


###  http://www.owl-ontologies.com/VirtualHome.owl#SocialInteraction
ho:SocialInteraction rdf:type owl:Class ;
                     rdfs:subClassOf ho:Activity ;
                     rdfs:comment """Class that contains sequences that are related between , human-robot or 
human-human interaction.""" .


###  http://www.owl-ontologies.com/VirtualHome.owl#Stretch
ho:Stretch rdf:type owl:Class ;
           rdfs:subClassOf ho:PhysicalActivity ;
           rdfs:comment "A class with soft physical activity, or exercise, such as stretch." .


###  http://www.owl-ontologies.com/VirtualHome.owl#Wait
ho:Wait rdf:type owl:Class ;
        rdfs:subClassOf ho:Other ;
        rdfs:comment "The class with sequence of actions for waiting." .


